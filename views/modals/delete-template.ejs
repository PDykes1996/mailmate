<dialog id="delete-template" style="max-width: 24rem">
	<form action="/template/_method=DELETE" method="post" class="flow">
		<header>
			<h1 class="fs:xl">Delete Template</h1>
		</header>
		<div class="cluster">
			<p>Are you sure you want to delete this template?</p>
		</div>
		<footer class="cluster">
			<div style="margin-left: auto">
				<button type="button" class="secondary" data-close>Cancel</button>
				<button type="submit" class="error">Delete</button>
			</div>
		</footer>
	</form>
</dialog>
